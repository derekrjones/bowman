{
  "name": "test",
  "version": "0.0.0",
  "dependencies": {
    "angular": "~1.3.7",
    "bootstrap": "~3.3.1"
  },
  "bowman": {
    "debug": true,
    "plugins": [
      "bowman-angular",
      "bowman-exports"
    ],
    "scanner": {
      "verbose": true
    },
    "modifiers": {
      "main": {
        "angular-ui-bootstrap": [],
        "async": "lib/async.js",
        "jshint": "dist/jshint.js",
        "labjs": "LAB.min.js",
        "q": "q.js",
        "scriptjs": "dist/script.js",
        "bootstrap-material-design": [
          "dist/css/material-wfont.min.css"
        ],
        "d3-plugins": [
          "hexbin/hexbin.js"
        ],
        "colorbrewer": "colorbrewer.js",
        "prism": [
          "!themes/prism.css",
          "themes/prism-okaidia.css",
          "components/prism-javascript.js",
          "plugins/line-numbers/prism-line-numbers.js",
          "plugins/line-numbers/prism-line-numbers.css"
        ],
        "smoothwheel": "src/jquery.smoothwheel.js"
      },
      "dependencies": {
        "angular-bootstrap": {
          "bootstrap": "*"
        },
        "ng-scrollbar": {
          "angular": "*"
        },
        "ngprogress": {
          "angular": "*"
        },
        "d3-plugins": {
          "d3": "*"
        },
        "smoothwheel": {
          "jquery": "*"
        }
      }
    },
    "bowman-exports": {
      "angular-scroll": "angular.module('duScroll')",
      "ng-Fx": "angular.module('ngFx')"
    }
  }
}
